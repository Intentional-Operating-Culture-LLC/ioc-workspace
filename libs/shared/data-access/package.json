  "peerDependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "@supabase/ssr": "^0.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "^15.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
