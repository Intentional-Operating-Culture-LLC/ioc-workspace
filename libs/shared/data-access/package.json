  "dependencies": {
    "date-fns": "^4.1.0",
    "socket.io-client": "^4.7.0",
    "axios": "^1.6.0",
    "@supabase/supabase-js": "^2.50.0",
    "@supabase/ssr": "^0.6.1",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0"
