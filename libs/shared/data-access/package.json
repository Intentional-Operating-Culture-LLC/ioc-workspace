    "./performance/*": {
      "import": "./dist/performance/*.js",
      "require": "./dist/performance/*.js",
      "types": "./dist/performance/*.d.ts"
    },
    "./zoho": {
      "import": "./dist/zoho/index.js",
      "require": "./dist/zoho/index.js",
      "types": "./dist/zoho/index.d.ts"
    },
    "./zoho/*": {
      "import": "./dist/zoho/*.js",
      "require": "./dist/zoho/*.js",
      "types": "./dist/zoho/*.d.ts"
    }
