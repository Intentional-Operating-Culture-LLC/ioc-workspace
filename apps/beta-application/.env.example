# Beta Application Environment Variables
# Copy this file to .env.local for local development

# Application
NEXT_PUBLIC_APP_NAME=IOC Beta
NEXT_PUBLIC_APP_URL=http://localhost:3009
NODE_ENV=staging

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3009/api
API_BASE_URL=http://localhost:3009

# Supabase (Beta Environment)
NEXT_PUBLIC_SUPABASE_URL=your_beta_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_beta_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_beta_service_role_key

# Authentication
NEXTAUTH_URL=http://localhost:3009
NEXTAUTH_SECRET=your_beta_nextauth_secret
BETA_ACCESS_PASSWORD=your_beta_password

# Database (Beta)
DATABASE_URL=postgresql://user:password@localhost:5432/ioc_beta

# Feature Flags
NEXT_PUBLIC_ENABLE_OCEAN_ASSESSMENT=true
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
NEXT_PUBLIC_ENABLE_BETA_FEATURES=true
NEXT_PUBLIC_ENABLE_EXPERIMENTAL_FEATURES=true

# Beta Testing
BETA_TESTER_EMAILS=beta-testers@iocframework.com
ENABLE_FEATURE_VOTING=true
ENABLE_FEEDBACK_WIDGET=true
ENABLE_A_B_TESTING=true

# Analytics (Beta)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-BETA-XXXXX
NEXT_PUBLIC_MIXPANEL_TOKEN=your_beta_mixpanel_token
ENABLE_SESSION_RECORDING=true

# Error Tracking
SENTRY_DSN=your_beta_sentry_dsn
SENTRY_ENVIRONMENT=beta
LOG_LEVEL=verbose

# Performance
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_BUNDLE_ANALYZER=true

# Deployment
VERCEL_URL=
VERCEL_ENV=preview